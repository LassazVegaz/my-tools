{
  "name": "my-tools",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "deploy": "dotenv -e .env.deploy.local -c -- next build",
    "deploy:start": "dotenv -e .env.deploy.local -c -- next start",
    "prisma:migrate": "dotenv -e .env.development.local -c -- prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env.production.local -c -- prisma migrate deploy",
    "scripts:copy-db": "dotenv -e scripts/.env.db-copy.local -- ts-node scripts/db-copy.ts"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.6",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@mui/x-charts": "^6.19.8",
    "@mui/x-date-pickers": "^6.19.9",
    "@prisma/client": "5.8.1",
    "dayjs": "^1.11.11",
    "fk-mui": "^1.0.6",
    "formik": "^2.4.6",
    "next": "14.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^10.0.5",
    "sass": "^1.75.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.6",
    "postcss": "^8.4.38",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0"
  }
}
